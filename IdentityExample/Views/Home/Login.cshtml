<form asp-action="Register" method="post" class="col-6">
    <div class="form-group">
        <label for="exampleInputEmail1">Username</label>
        <input type="email" class="form-control" name="username" value="@ViewBag.username">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input type="password" class="form-control" name="password" value="@ViewBag.password">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>